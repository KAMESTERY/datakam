{
  "name": "controlkam",
  "profile": "default",
  "regions": [
    "us-east-1"
  ],
  "lambda": {
    "memory": 1024,
    "warm": true,
    "warm_count": 10,
    "warm_rate": "30m",
    "policy": [
      {
        "Sid": "AllowCloudFrontUpdateDistribution",
        "Effect": "Allow",
        "Action": [
          "cloudfront:updateDistribution"
        ],
        "Resource": [
          "*"
        ]
      }
    ]
  },
  "stages": {
    "ck_prod": {
      "domain": "ctrlkm.kamestery.com"
    },
    "ck_stage": {
      "domain": "ctrlkm-stage.kamestery.com"
    },
    "ck_dev": {
      "domain": "ctrlkm-dev.kamestery.com"
    }
  },
  "environment": {
    "LOG_LEVEL": "debug"
  }
}
